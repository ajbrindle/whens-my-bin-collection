  "settings": {},
  "theme": "dark",
  "import": [
    {
      "name": "alexa-layouts",
      "version": "1.5.0"
    }
  ],
  "resources": [],
  "styles": {},
  "onMount": [],
  "graphics": {},
  "commands": {},
  "layouts": {},
  "mainTemplate": {
    "parameters": [
      "binDisplayData"
    ],
    "items": [
      {
        "type": "Container",
        "width": "100vw",
        "height": "100vh",
        "direction": "column",
        "justifyContent": "center",
        "alignItems": "center",
        "items": [
          {
            "type": "Text",
            "fontSize": "24dp",
            "paddingLeft": "@marginHorizontal",
            "paddingTop": "@spacingLarge",
            "paddingRight": "@marginHorizontal",
            "text": "${binDisplayData.text}",
            "textAlign": "center"
          },
          {
            "type": "AlexaImage",
            "imageSource": "${binDisplayData.imageSource}",
            "width": "100vw",
            "imageAlign": "center",
            "imageScale": "best-fit"
          }
        ]
      }
    ]
  }
}